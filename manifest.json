{
  "manifest_version": 2,

  "name": "Left-to-right Flipper",
  "version": "0.4.0",
  "description": "Flips the web left to right",

  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["content.js"],
    "matches": [
      "https://notion.so/*",
      "https://www.notion.so/*",
      "https://meet.google.com/*"
    ]
  }]

}
